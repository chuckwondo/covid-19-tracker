n#country2 p {\n  color: #008000;\n  font-weight: 600;\n}\n\n.hide {\n  display: none;\n}\n\na {\n  text-decoration: none;\n}\n\nli,\np {\n  color: #f5f5dc;\n}\n\n#social {\n  padding-bottom: 20px;\n  padding-top: 10px;\n}\n\n#social a i {\n  padding: 15px;\n  font-size: 1.5rem;\n  color: #000000;\n  text-align: center;\n}\n\nfooter {\n  background-color: rgba(255, 255, 255, 0.5);\n  padding-right: 10px;\n}\n\nfooter p {\n  background-color: rgba(255, 255, 255, 0.5);\n  border-radius: 5px;\n  padding-top: 5px;\n  padding-bottom: 5px;\n  margin-top: 8px;\n  margin-bottom: 8px;\n}\n\nfooter p a {\n  color: #000000;\n\n}\n\n@media only screen and (max-width: 769px) {\n  #social {\n    text-align: center;\n  }\n}\n\n@media only screen and (min-width: 769px) {\n  p {\n    width: 80%;\n  }\n}"},"lineCount":null}},"error":null,"hash":"82c90727777402a1bfd92919d5138dc8","cacheData":{"env":{}}}